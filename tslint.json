{
  "extends": "tslint-config-prettier",
  "rules": {
    "typedef": [true, "call-signature"],
    "forin": true,
    "member-access": true,
    "no-bitwise": true,
    "no-construct": true,
    "no-debugger": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-var-requires": true
  }
}
